{
  "name": "nexus-plugin-prisma",
  "version": "0.0.0-dripip",
  "license": "MIT",
  "author": "Jason Kuhrt",
  "main": "dist/runtime.js",
  "module": "dist/mylib.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "global-type.d.ts"
  ],
  "scripts": {
    "dev": "tsc --watch",
    "build": "yarn clean && node build-scripts/update-hash.js && tsc",
    "build:ci": "tsc",
    "test:e2e:mysql": "yarn jest test/e2e/mysql.test.ts --testTimeout=400000",
    "test:e2e:postgres": "yarn jest test/e2e/postgres.test.ts --testTimeout=400000",
    "test:e2e:sqlite": "yarn jest test/e2e/sqlite.test.ts --testTimeout=400000",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "release:stable": "dripip stable",
    "release:preview": "dripip preview",
    "prepack": "yarn -s build",
    "postinstall": "mkdir -p \"$PWD/../@types/nexus-plugin-prisma\" && cp \"$PWD/global-type.d.ts\" \"$PWD/../@types/nexus-plugin-prisma/index.d.ts\""
  },
  "peerDependencies": {
    "nexus-future": "^0.12.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --pattern '**/*.ts'"
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@types/common-tags": "1.8.0",
    "@types/jest": "25.1.4",
    "@types/node": "^13.9.3",
    "@types/prismjs": "1.16.0",
    "dripip": "0.8.0",
    "husky": "4.2.3",
    "jest": "^25.1.0",
    "nexus-future": "^0.0.0-pr.534.1.b4b5b6b",
    "node-fetch": "2.6.0",
    "node-pty": "0.9.0",
    "prettier": "1.19.1",
    "pretty-quick": "2.0.1",
    "ts-jest": "^25.2.1",
    "typescript": "3.8.3"
  },
  "dependencies": {
    "@prisma/client": "2.0.0-preview023",
    "@prisma/generator-helper": "0.0.43",
    "@prisma/sdk": "0.0.222",
    "@prisma/studio-server": "0.204.0",
    "chalk": "^3.0.0",
    "common-tags": "^1.8.0",
    "fs-jetpack": "^2.2.3",
    "get-port": "^5.1.1",
    "nexus": "^0.12.0-rc.13",
    "nexus-prisma": "^0.11.1",
    "open": "^7.0.3",
    "prisma2": "2.0.0-preview023",
    "prismjs": "^1.19.0",
    "stacktrace-parser": "^0.1.9"
  },
  "prisma": {
    "version": "d3b0ceed5d87544b9d2decb70e08664f9047bb73"
  },
  "jest": {
    "preset": "ts-jest"
  }
}
